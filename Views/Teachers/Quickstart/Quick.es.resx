  <data name="Block4Title" xml:space="preserve">
    <value>Tus propios horarios</value>
  </data>
  <data name="Block5Subtitles" xml:space="preserve">
    <value>Tú decides cuánto ganarás y el número de horas que inviertes en la plataforma</value>
